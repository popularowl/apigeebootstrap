<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="default">    
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request/>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>  
    
    <HTTPTargetConnection>        
        <Properties>
            <!-- enable manual handling of error codes -->
            <Property name="success.codes">1xx,2xx,4xx,5xx</Property>
        </Properties>
        
        <URL>https://httpbin.org</URL>
        
        <!-- or can be owverwritten by poplicy
        <URL>http://overwriten-by-policy-.net</URL>
        -->
    </HTTPTargetConnection>

</TargetEndpoint>